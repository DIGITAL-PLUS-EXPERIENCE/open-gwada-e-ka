    "start": "npm run dev",
    "start-remote": "vite --host"
  },
  "author": "Xavier NICOLZA",
  "license": "GNUv3",
  "dependencies": {
    "@vaadin/router": "^1.7.4",
    "lit": "^2.3.1"
  },
  "devDependencies": {
    "typescript": "^4.6.3",
    "vite": "^2.9.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
